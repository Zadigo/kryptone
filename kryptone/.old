def build_headers(self, options):
    headers = {
        'User-Agent': RANDOM_USER_AGENT(),
        'Accept-Language': 'en-US,en;q=0.9'
    }
    items = [f"--header={key}={value})" for key, value in headers.items()]
    options.add_argument(' '.join(items))


